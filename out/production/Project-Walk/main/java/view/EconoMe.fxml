<BorderPane prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.FXMLDocumentController">
   <left>
      <HBox prefHeight="680.0" prefWidth="255.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="680.0" prefWidth="310.0">
               <children>
                  <Pane minHeight="51.0" minWidth="72.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="51.0" prefWidth="218.0" scaleShape="false">
                     <children>
                        <JFXHamburger fx:id="hamburger" layoutX="210.0" layoutY="2.0" prefHeight="47.0" prefWidth="45.0" />
                        <Label fx:id="mainLabel" layoutX="-2.0" layoutY="-1.0" prefHeight="57.0" prefWidth="201.0" text="Select Country">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </Pane>
                  <StackPane fx:id="stack" VBox.vgrow="ALWAYS">
                     <children>
                        <JFXDrawer fx:id="drawer" defaultDrawerSize="200.0" prefHeight="610.0" prefWidth="218.0" />
                        <VBox id="listID" minWidth="213.0" prefHeight="645.0" prefWidth="218.0">
                           <children>
                              <ListView fx:id="list" onKeyReleased="#handleListAction2" onMouseClicked="#handleListAction" prefHeight="680.0" prefWidth="255.0" stylesheets="@style.css" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                     </children>
                  </StackPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </left>
   <center>
      <JFXTabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" stylesheets="@style.css" tabClosingPolicy="UNAVAILABLE" tabMinHeight="50.0" BorderPane.alignment="CENTER">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <tabs>
            <Tab id="gdp" fx:id="tabCurrGDP" text="GDP">
               <content>
                  <VBox prefHeight="538.0" prefWidth="795.0">
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>
                        <LineChart fx:id="lineChartCurrGDP" prefHeight="599.0" prefWidth="549.0" VBox.vgrow="ALWAYS">
                           <xAxis>
                              <CategoryAxis side="BOTTOM" fx:id="xAxisCurrGDP" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis fx:id="yAxisCurrGDP" side="LEFT" />
                           </yAxis>
                        </LineChart>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab id="gdpcapita" fx:id="tabGDPCapita" text="GDP Per Capita">
               <content>
                  <VBox>
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>                       <LineChart fx:id="lineChartGDPCapita" prefHeight="627.0" prefWidth="823.0" VBox.vgrow="ALWAYS">
                          <xAxis>
                             <CategoryAxis side="BOTTOM" fx:id="xAxisGDPCapita" />
                          </xAxis>
                          <yAxis>
                             <NumberAxis fx:id="yAxisGDPCapita" side="LEFT" />
                          </yAxis>
                       </LineChart>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab id="inflation" fx:id="tabInflation" text="Inflation">
              <content>
                  <VBox>
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>
                       <LineChart fx:id="lineChartInflation" VBox.vgrow="ALWAYS">
                          <xAxis>
                             <CategoryAxis side="BOTTOM" fx:id="xAxisInflation" />
                          </xAxis>
                          <yAxis>
                             <NumberAxis fx:id="yAxisInflation" side="LEFT" />
                          </yAxis>
                       </LineChart>
                     </children>
                  </VBox>
              </content>
            </Tab>
            <Tab id="unemployment" fx:id="tabUnemployment" text="Unemployment">
              <content>
                  <VBox>
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>
                       <LineChart fx:id="lineChartUnemployment" VBox.vgrow="ALWAYS">
                          <xAxis>
                             <CategoryAxis side="BOTTOM" fx:id="xAxisUnemployment" />
                          </xAxis>
                          <yAxis>
                             <NumberAxis fx:id="yAxisUnemployment" side="LEFT" />
                          </yAxis>
                       </LineChart>
                     </children>
                  </VBox>
              </content>
            </Tab>
            <Tab id="gdpGrowth" fx:id="tabGDPGrowth" text="GDP Growth">
               <content>
                  <VBox>
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>                        <LineChart fx:id="lineChartGDPGrowth" VBox.vgrow="ALWAYS">
                           <xAxis>
                              <CategoryAxis side="BOTTOM" fx:id="xAxisGDPGrowth" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis fx:id="yAxisGDPGrowth" side="LEFT" />
                           </yAxis>
                        </LineChart>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab id="gdpGrowthCapita" fx:id="tabGDPGrowthCapita" text="GDP Per Capita Growth">
               <content>
                  <VBox>
                     <children>
                        <VBox alignment="TOP_RIGHT" prefHeight="37.0" prefWidth="347.0" styleClass="download" stylesheets="@style.css">
                           <children>
                              <JFXButton id="download" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleDownload" prefHeight="34.0" prefWidth="40.0" styleClass="download" stylesheets="@style.css" text=" ">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin></JFXButton>
                           </children>
                        </VBox>                        <LineChart fx:id="lineChartGDPGrowthCapita" VBox.vgrow="ALWAYS">
                           <xAxis>
                              <CategoryAxis side="BOTTOM" fx:id="xAxisGDPGrowthCapita" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis fx:id="yAxisGDPGrowthCapita" side="LEFT" />
                           </yAxis>
                        </LineChart>
                     </children>
                  </VBox>
               </content>
            </Tab>
          <Tab fx:id="details" text="Details">
               <content>
                  <TableView fx:id="detailTable" prefHeight="200.0" prefWidth="200.0" stylesheets="@style.css">
                     <!--
                    <columns>
                      <TableColumn prefWidth="137.0" text="Country" fx:id="countryColumn"/>
                      <TableColumn prefWidth="91.0" text="Year" fx:id="yearColumn"/>
                       <TableColumn minWidth="0.0" prefWidth="185.0" text="GDP ($)" fx:id="gdpColumn"/>
                       <TableColumn prefWidth="191.0" text="GDP Per Capita ($)" fx:id="gdpPerCapitaColumn"/>
                       <TableColumn prefWidth="142.0" text="Inflation Rate (%)" fx:id="inflationRateColumn"/>
                       <TableColumn prefWidth="186.0" text="Unemployment Rate (%)" fx:id="unemploymentRateColumn"/>
                       <TableColumn prefWidth="186.0" text="GDP Growth (%)" fx:id="gdpGrowthColumn"/>
                       <TableColumn prefWidth="186.0" text="GDP Per Capita Growth (%)" fx:id="gdpPerCapitaGrowthColumn"/>
                    </columns>
                     -->
                  </TableView>
               </content>
          </Tab>
         </tabs>
      </JFXTabPane>
   </center>
</BorderPane>
